    "@testing-library/react": "^14.0.0",
    "@types/axios": "^0.14.0",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^4.0.0",
